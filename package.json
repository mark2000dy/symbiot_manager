{
  "name": "gastos-symbiot-app",
  "version": "1.0.0",
  "description": "PWA para control de gastos e ingresos - Symbiot Technologies",
  "main": "server/app.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "build": "echo Building for production...",
    "test": "node test.js",
    "test:db": "node test-db.js",
    "test:api": "node test-api-simple.js",
    "diagnose": "node diagnose-appserv.js",
    "setup-appserv": "node setup-appserv.js",
    "setup-db": "node setup-appserv.js && node database/seed.js",
    "seed": "node seed.js",
    "seed:excel": "node database/seed-from-excel.js",
    "fresh": "npm run setup-appserv && npm run seed && npm run dev",
    "reset-data": "npm run seed"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.2",
    "mysql2": "^3.6.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "gastos",
    "ingresos",
    "pwa",
    "symbiot",
    "node.js",
    "appserv",
